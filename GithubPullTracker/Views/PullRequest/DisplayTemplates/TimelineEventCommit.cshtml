@model TimelineEventCommit




<div class="event">
    <i class="icon icon-chevron-right"></i> <a href="@Model.CreatedBy.html_url" target="_blank">@Model.CreatedBy.login</a> added some commits <time class="timeago" datetime="@Model.CreatedAt.ToString("o")">@Model.CreatedAt.ToString()</time>
</div>
<div class="commit-list">
    
    @foreach (var b in Model.Items)
    {
        <div class="commit ">
                <a href="@b.author.html_url" target="_blank">
                    <img src="@(b.author.avatar_url)&s=40" class="avatar" />
                </a>
                <a href="@b.author.html_url" target="_blank">@b.author.login</a>
                @b.commit.message
            </div>
    }
</div>