@model TimelineEventCommit




<div class="commit-list event">
    <div class="header">
        <a href="@Model.CreatedBy.html_url" target="_blank">
            <img src="@(Model.CreatedBy.avatar_url)&s=40" class="avatar" alt="@Model.CreatedBy.login" />
        </a>
        <a href="@Model.CreatedBy.html_url" target="_blank">@Model.CreatedBy.login</a> added some commits <time class="timeago" datetime="@Model.CreatedAt.ToString("o")">@Model.CreatedAt.ToString()</time>
    </div>

    
    @foreach (var b in Model.Items)
    {
        <div class="commit ">
                <a href="@b.author.html_url" target="_blank">
                    <img src="@(b.author.avatar_url)&s=40" class="avatar" />
                </a>
                <a href="@b.author.html_url" target="_blank">@b.author.login</a>
                @b.commit.message
            </div>
    }
</div>